{
    "name": "johncarter/filament-focal-point-picker",
    "description": "An image focal point picker for Filament Admin.",
    "keywords": [
        "johncarter",
        "laravel",
        "filament-focal-point-picker"
    ],
    "homepage": "https://github.com/johncarter/filament-focal-point-picker",
    "license": "MIT",
    "authors": [
        {
            "name": "John Carter",
            "email": "john@johncarter.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.0|^4.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Johncarter\\FilamentFocalPointPicker\\": "src"
        }
    },
    "scripts": [],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Johncarter\\FilamentFocalPointPicker\\FilamentFocalPointPickerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
